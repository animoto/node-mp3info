{
  "name": "mp3info",
  "description": "NodeJS native bindings to mp3info.",
  "keywords": [
    "mp3info",
    "mp3",
    "mpeg",
    "mpg",
    "id3",
    "duration"
  ],
  "version": "0.0.8",
  "author": "Marek BÄ™dkowski <marek@bedkowski.pl> (http://thejsteam.com)",
    "repository": {
    "type": "git",
    "url": "git://github.com/bendi/node-mp3info.git"
  },
  "main": "./index.js",
  "dependencies": {
    "bindings": "*",
    "debug": "*",
    "node-pre-gyp": "0.5.5"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build"
  },
  "bundledDependencies":["node-pre-gyp"],
  "binary": {
    "module_name": "bindings",
    "module_path": "./build/Release",
    "host": "https://mp3info.s3.amazonaws.com",
    "remote_path" : "./{configuration}/"
  }
}
